generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model profiles {
  birthdate    DateTime
  phone_number String
  user_id      Int      @id
  username     String
  users        users    @relation(fields: [user_id], references: [id])
}

model users {
  email    String     @unique
  id       Int        @default(autoincrement()) @id
  profiles profiles[]
}
